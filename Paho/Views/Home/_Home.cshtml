                            <select id="nativepeople" class="form-control" data-bind="options: CNP,
                                            optionsText: 'Name',
                                            optionsValue: 'Id',
							                optionsCaption: '@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelEthnicityLabel',
                                            value: selectedNativepeopleID"></select>
                        </div>
                    </div>
                </div>


                <div class="row rowform">
                    <div class="col-md-12">

