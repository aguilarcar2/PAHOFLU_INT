            <span></span>
            <button id="cancel" type="button" class="btn btn-primary" data-bind="click: Cancel, visible:false">Regresar</button>
            @if (ViewBag.UserInstitutionType != 2)
            {
                <span></span>
                <button id="Reset" type="button" class="btn btn-primary" data-bind="click: AddFluCase, visible:false">Nuevo registro</button>
            }
            <span></span>
            <button id="Borrar" type="button" class="btn btn-primary" data-bind="click: Borrar, visible:false">Borrar</button>
