                function loadData(id) {
                    //alert(id);
                    app.Views.Home.EditFluCase();
                    app.Views.Contact.GetContact(id);
                    app.Views.Home.FlowData();
                    app.Views.GEO.GetGEO(id);
                    app.Views.Risk.GetRisk(id);
                    app.Views.Hospital.GetHospital(id);
                    app.Views.Home.FlowDataLab();
