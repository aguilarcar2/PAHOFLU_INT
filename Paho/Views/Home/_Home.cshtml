@model Paho.Models.CaseViewModel
<!-- ko with: home -->
<div class="row">
    <div class="col-md-12" data-bind="with: app.Views.Home">
        <div class="row" style="margin-bottom:7px; margin-top:1em;">
            @if (Model.DisplayCountries)
            {
                <div class='input-group col-md-7' style="margin-top: 3px; display: none; float: left" data-bind="visible: displayFilters">
                    <span class="input-group-addon">
                        País:
                    </span>
                    <select id="Pais" class="form-control" data-bind="options: activecountries,
                           optionsText: 'Name',
                           optionsValue: 'Id',
                           value: selectedCountryId,
                           event:{ change: ReloadInstitutions}"></select>
                </div>
            }
            @if (Model.DisplayRegionals)
            {
                <div id="RegionalsGroup" class='input-group col-md-7' style="margin-top: 7px; display: none; float: left" data-bind="visible: displayFilters">
                    <span class="input-group-addon">
                        @ResourcesM.ResourcesM.viewHome_msgRegionals
                    </span>
                    <select id="Regionals" class="form-control" data-bind="options: regions,
                       optionsText: 'Name',
                       optionsValue: 'Id',
                       value: selectedRegionId,
                       event:{ change: ReloadFluCases}"></select>
                </div>
            }
            @if (Model.DisplayHospitals)
            {
                <div id="HospitalsGroup" class='input-group col-md-7' style="margin-top: 7px; display: none; float: left" data-bind="visible: displayFilters">
                    <span class="input-group-addon">
                        @ResourcesM.ResourcesM.viewHome_msgHospitals
                    </span>
                    <select id="Hospitals" class="form-control" data-bind="options: institutions,
                       optionsText: 'Name',
                       optionsValue: 'Id',
                       value: selectedInstitutionId,
                       event:{ change: ReloadFluCases}"></select>
                </div>
            }
            @if (Model.DisplayLabs)
            {
                <div id="LabsGroup" class='input-group col-md-7' style="margin-top: 7px; display: none; float: left" data-bind="visible: displayFilters">
                    <span class="input-group-addon">
                        @ResourcesM.ResourcesM.viewHome_msgLabs
                    </span>
                    <select id="Labs" class="form-control" data-bind="options: institutions,
                            optionsText: 'Name',
                            optionsValue: 'Id',
                            value: selectedInstitutionId,
                            event:{ change: ReloadFluCases}"></select>
                </div>
            }

        </div>
        <div class="row" style="margin-bottom:7px; margin-top:7px;" data-bind="visible: shouldcaselist">
            <div class="col-md-4">
                <div class='input-group'>
                    <span class="input-group-addon">
                       @ResourcesM.ResourcesM.viewHome_msgSCasenumber
                    </span>
                    <input id="SCasenumber" type='text' class="form-control" data-bind="value:SCasenumber" />
                </div>
            </div>
            <!-- div class="col-md-3">
                <div class='input-group'>
                    <span class="input-group-addon">
                        Hospital reg.:
                    </span>
                    <input id="SHospRegis" type='text' class="form-control" />
                </div>
            </div -->
            <div class="col-md-4">
                <div class='input-group'>
                    <span class="input-group-addon">
                        @ResourcesM.ResourcesM.viewHome_msgSCaseStatus
                    </span>
                    <select id="SCaseStatus" class="form-control">
                        <option value=""></option>
                        <option value="A">Dado de alta</option>
                        <option value="D">Fallecido</option>
                        <option value="R">Referido a otro Centro</option>
                        <option value="O">Otro</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="row" style="margin-bottom:7px; margin-top:7px;" data-bind="visible: shouldcaselist">
            <div class="col-md-4">
                <div class='input-group'>
                    <span class="input-group-addon">
                        @ResourcesM.ResourcesM.viewHome_msgSName
                    </span>
                    <input id="SName" type='text' class="form-control" />
                </div>
            </div>

            <div class="col-md-4">
                <div class='input-group'>
                    <span class="input-group-addon">
                        @ResourcesM.ResourcesM.viewHome_msgSLastName
                    </span>
                    <input id="SLastName" type='text' class="form-control" />
                </div>
            </div>
        </div>
        @if (ViewBag.UsrCtry != 7)
        {
        <div class="row" style="margin-bottom:7px; margin-top:7px;" data-bind="visible: shouldcaselist">
            <div class="col-md-4">
                <div class='input-group'>
                    <span class="input-group-addon">
                       @ResourcesM.ResourcesM.viewHome_msgSNStartDate
                    </span>
                    <input id="SNStartDate" type='text' class="form-control" data-bind="datepicker: SNStartDate" />
                </div>
            </div>
            <div class="col-md-4">
                <div class='input-group'>
                    <span class="input-group-addon">
                        @ResourcesM.ResourcesM.viewHome_msgSNEndDate
                    </span>
                    <input id="SNEndDate" type='text' class="form-control" data-bind="datepicker: SNEndDate" />
                </div>
            </div>

        </div>
        }
        <div class="row" style="margin-bottom:7px; margin-top:7px;" data-bind="visible: shouldcaselist">
            <div class="col-md-4">
                <div class='input-group'>
                    <span class="input-group-addon">
                        @ResourcesM.ResourcesM.viewHome_msgSRecordId
                    </span>
                    <input id="SRecordId" type='text' class="form-control" />
                </div>
            </div>

            <div class="col-md-4">
                <div class='input-group'>
                    <button id="search" type="button" class="btn btn-primary" data-bind="click: SearchFlucases">@ResourcesM.ResourcesM.viewHome_btnSearch
</button>
                </div>
            </div>
        </div>

        </div>
</div>

@if (ViewBag.UserInstitutionType != 2)
{
    <div class="row" style="margin-top:1em;" data-bind="visible: newButton">
        <div class="col-md-8">
            <div class="btn-group">
                <button id="nuevo" type="button" class="btn btn-primary" data-bind="click: NewFluCase">@ResourcesM.ResourcesM.viewHome_btnNew</button>
            </div>
        </div>
    </div>
}

<div class="row">
    <div class="col-md-12" data-bind="visible: shouldcaselist" style="margin-top: 1em;">
        <table id="caselist"></table>
        <div id="caselistPager"></div>
    </div>
</div>

<div class="row">
    <div class="col-md-12" data-bind="visible: shouldcaselist" style="margin-top: 1em;">
        <table id="caselist_pend"></table>
        <div id="caselistPager_pend"></div>
    </div>
</div>



<div id="tabs" data-bind="visible: shouldtabs" style="margin-top: 7px; display: none">

            <ul>

                <li><a href="#tab-contact">@ResourcesM.ResourcesM.msgCaselistVigTabContact</a></li>
                <li><a href="#tab-GEO">@ResourcesM.ResourcesM.msgCaselistVigTabGeo</a></li>
                @*<!--NOTA: Arreglar para <li><a href="#tab-risk">@((ViewBag.UsrCtry == 3) ? "Antecedentes" : "Hist. médico") </a></li>-->*@
                <li><a href="#tab-risk">@ResourcesM.ResourcesM.msgCaselistVigTabRisk</a></li>
                <li><a href="#tab-hospital">@ResourcesM.ResourcesM.msgCaselistVigTabHospital</a></li>
                <li><a href="#tab-lab">@ResourcesM.ResourcesM.msgCaselistVigTabLab</a></li>
                <li><a href="#tab-case" data-bind="visible:true">@ResourcesM.ResourcesM.msgCaselistVigTabCase</a></li>
                <li><a href="#tab-history" data-bind="visible:true">@ResourcesM.ResourcesM.msgCaselistVigTabHistory</a></li>
                <text class="col-md-2" style="float: right; padding: 0.5em 1em; color:royalblue;">@ResourcesM.ResourcesM.msgCaselistVigTabRecord <text style="width:15px;color: royalblue ;" type="text" id="RecordNumber" disabled >1</text></text>
            </ul>

            <div id="tab-contact" data-bind="with: app.Views.Contact">

                <div class="row rowform">
                    <div class="col-md-6">
                        <span>
                            @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelSurv: <font color="#FF0000">*</font>
                        </span>
                        @if (ViewBag.SARI == true)
                        {
                            <label class="btn btn-default">
                                <input type="radio" name="IsSurv" id="IsSurv" value="1" data-bind="checked: IsSurv"> @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelSari
                            </label>
                        }
                        @if (ViewBag.ILI == true)
                        {
                        <label class="btn btn-default" >
                            <input type="radio" name="IsSurv" id="IsSurv" value="2" data-bind="checked: IsSurv"> @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelIli
                        </label>
                        }
                        <label class="btn btn-default">
                            <input type="checkbox" name="SurvInusual" id="SurvInusual" data-bind="checked: SurvInusual"> @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelUnusualCase
                        </label>
                        @if (ViewBag.UsrCtry == 9) {
                        <label class="btn btn-default">
                            <input type="checkbox" name="Brote" id="Brote" data-bind="checked: Brote"> @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelOutbreak
                        </label>
                        }

                    </div>
                    <div class="col-md-6" data-bind="visible:VisHopName">
                        <div class='input-group'>
                            <span class="input-group-addon">
                                @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelHospital
                            </span>
                            <span id="hospitalName" class="form-control" data-bind="text:hospitalName" disabled  />
                        </div>

                    </div>
                </div>
                @if (ViewBag.UsrCtry == 9)
                {
                    <div class="row rowform">

                        <div class="col-md-4">
                            <div class='input-group'>
                                <span class="input-group-addon">
                                    @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelRegionMINSA
                                </span>
                                <span id="region_salud" class="form-control" data-bind="text:region_salud" disabled />
                            </div>

                        </div>
                        <div class="col-md-2"></div>
                            <div class="col-md-4">
                                <div class='input-group'>
                                    <span class="input-group-addon">
                                        @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelRegionCCSS
                                    </span>
                                    <span id="region_institucional" class="form-control" data-bind="text:region_institucional" disabled />
                                </div>

                            </div>
                        </div>
                }
                <div class="row rowform">
                        <div class="col-md-6">
                            <div class='input-group'>
                                <span class="input-group-addon" data-bind="visible:SurvSARI">
                                  @(((ViewBag.ILI == true && ViewBag.SARI == true) || (ViewBag.SARI == true)) ? (ViewBag.UsrCtry == 3) ? "@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelDateOfCollect" : ("@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelDateOfNotification") : ("@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelDateOfCollect")): <font color="#FF0000">*</font>
                                </span>
                                <span class="input-group-addon" data-bind="visible:SurvILI">
                                    @((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ? "@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelDateOfCollect" : "@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelDateOfDetection"): <font color="#FF0000">*</font>
                                </span>
                                <input id="HospitalDate" type="text" class="form-control " data-bind='datepicker: HospitalDate, datepickerOptions: { maxDate:@Model.DatePickerConfig,  dateFormat:"@Model.DateFormatDP", autoHide:false}' />
                                <label class="date_format">dd/mm/yyyy</label>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class='input-group'>
                                <span class="input-group-addon">
                                    @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelRecordDate
                                </span>
                                <input id="RegDate" type='text' class="form-control " data-bind='datepicker: RegDate, datepickerOptions: { maxDate:@Model.DatePickerConfig,  dateFormat:"@Model.DateFormatDP"}'  readonly />
                                <label class="date_format">dd/mm/yyyy</label>
                            </div>
                        </div>
                    </div>

                    <div class="row rowform">
                        <div class="col-md-6">
                            <div class='input-group'>
                                <span class="input-group-addon">
                                    @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelIdType <font color="#FF0000">*</font>
                                </span>
                                <select id="DocumentType" class="form-control" data-bind="value:DocumentType">
                                @if (ViewBag.UsrCtry != 7)
                                {
                                    <option value=""></option>
                                    <option value="1">@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelDocumentName</option>
                                    <option value="2">@(ViewBag.UsrCtry == 9 ? "@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelForeigner" : "@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelPassport")</option>
                                    <option value="3">@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelInternalDocument</option>
                                    <option value="7" data-bind="visible:app.Views.Contact.ActiveBOL">@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelClinicalHistory</option>
                                    <option value="8" data-bind="visible:app.Views.Contact.ActiveBOL">@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelAnotherRecord</option>
                                }
                                else
                                {
                                    <option value=""></option>
                                    <option value="4">@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelRUN</option>
                                    <option value="5">@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelPassport</option>
                                    <option value="6">@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelOther</option>
                                }

                                </select>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class='input-group'>
                                @if (ViewBag.UsrCtry != 7)
                                {
                                    <span class="input-group-addon"> @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelIdNo <font color="#FF0000">*</font></span>
                                }
                                else
                                {
                                    <span class="input-group-addon"> @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelRUN: <font color="#FF0000">*</font></span>
                                }

                                <input id="NoExpediente" type='text' class="form-control case_letter" data-bind="value: NoExpediente, event: { blur: GetPatient }" />
                                @*<button id="DatosPaciente" type="button" class="btn btn-primary btn-next " data-bind="click: GetPatient, visible:EnableCR">Datos paciente</button>*@
                            </div>
                        </div>
                        <div class="col-md-6" style="display:none;">
                            <div class='input-group'>
                                <span class="input-group-addon">
                                    @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelHospitalRegNumber
                                </span>
                                <input id="Documento" type='text' class="form-control case_letter" data-bind="value: NationalId" />
                            </div>
                        </div>
                    </div>
                <div class="row rowform">
                    <div class="col-md-6">
                        <div class='input-group'>
                            <span class="input-group-addon">
                                @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelFirstLastName :<font color="#FF0000">*</font><br /><br />
                                @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelLastLastName :<font color='#FF0000' data-bind="visible:VisLName2Mand">*</font>
                            </span>
                            <input id="Apellidos1" type='text' class="form-control case_letter" data-bind='value: LName1' /><br />
                            <input id="Apellidos2" type='text' class="form-control case_letter" data-bind="value: LName2" />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class='input-group'>
                            <span class="input-group-addon">
                                @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelFirstFirstName :<font color="#FF0000">*</font><br /><br />@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelMiddleName :
                            </span>
                            <input id="Nombres1" type='text' class="form-control case_letter" data-bind='value: FName1' /><br />
                            <input id="Nombres2" type='text' class="form-control case_letter" data-bind="value: FName2" />

                        </div>
                    </div>

                </div>
                    <div class="row rowform">
                        <div class="col-md-6">
                            <div class='input-group'>
                                <span class="input-group-addon">
                                    @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelDOB <font color="#FF0000">*</font>
                                </span>
                                <input id="DOB" type='text' class="form-control" data-bind='valueUpdate:"change" , datepicker: DOB , datepickerOptions: { changeYear: true, yearRange: "-90:+0" , maxDate:@Model.DatePickerConfig,  dateFormat:"@Model.DateFormatDP"}' />
                                <input id="DOB_dummy" type='text' class="form-control" data-bind='datepicker: DOB , datepickerOptions: { changeYear: true, maxDate:@Model.DatePickerConfig,  dateFormat:"@Model.DateFormatDP"}, visible:false' />
                                <label class="date_format">dd/mm/yyyy</label>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class='input-group'>
                                <span class="input-group-addon">
                                   @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelAge <font color="#FF0000">*</font>
                                </span>
                                <input id="Age" type='text' class="form-control" data-bind="value:Age" />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class='input-group'>
                                <span class="input-group-addon">
                                    @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelYMorD <font color="#FF0000">*</font>
                                </span>
                                <select id="AMeasure" class="form-control" data-bind="value:AMeasure, event:{ change: CalculateDOB}">
                                    <option value=""></option>
                                    <option value="Year">@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelYear</option>
                                    <option value="Month">@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelMonth</option>
                                    <option value="Day">@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelDay</option>
                                </select>
                            </div>
                        </div>
                    </div>
                <div class="row rowform">
                    <div class="col-md-6" data-bind="visible:app.Views.Contact.NoActiveBOL">
                        <div class='input-group'>
                            <span class="input-group-addon">
                                @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelAgeGroup
                            </span>
                            <input id="AGroup" type='text' readonly="readonly" class="form-control" data-bind="value:AgeGroup" />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class='input-group'>
                            <span class="input-group-addon">
                                @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelSex <font color="#FF0000">*</font>
                            </span>
                            <label class="btn btn-default">
                                <input type="radio" name="options" id="Sex" value="Male" data-bind="checked: Gender"> @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelSexMale
                            </label>
                            <label class="btn btn-default">
                                <input type="radio" name="options" id="Sex" value="Female" data-bind="checked: Gender"> @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelSexFemale
                            </label>
                            <label class="btn btn-default">
                                <input type="radio" name="options" id="Sex" value="Unknown" data-bind="checked: Gender"> @(ViewBag.UsrCtry != 7 ? ResourcesM.ResourcesM.msgCaselistVigTabContactLabelSexUndefined : @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelSexUndetermined)
                            </label>
                        </div>
                    </div>

                </div>

                <div class="row rowform" data-bind="visible:SurvSARICHI">
                    <div class="col-md-6">
                        <div class='input-group'>
                            <span class='input-group-addon'>@ResourcesM.ResourcesM.msgCaselistVigTabContactLabelNacionality</span>
                            <select id="nationality" class="form-control" data-bind="options: app.Views.Home.countries,
                      optionsText: 'Name',
                      optionsValue: 'Id',
                      value: selectedNationalityID,
                      optionsCaption: '@ResourcesM.ResourcesM.msgSelectLabel'"></select>

                        </div>
                    </div>
                    <div class="col-md-6" data-bind="visible:NacionalidadCHI">
                        <div class='input-group'>
                            <span class="input-group-addon">
                                @ResourcesM.ResourcesM.msgCaselistVigTabContactLabelEthnicity
                            </span>
