        <span class="input-group-addon">
            <!--Grupo edad-->
            @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelAgeGroup"))
        </span>
    </div>

    <!-- Cabecera Datos -->
