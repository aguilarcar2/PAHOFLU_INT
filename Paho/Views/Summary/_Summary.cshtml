    <table class="table table-hover table-responsive" style="width:480px;">
        <thead id="LabelBandeja">
            <tr>
                <th class="col-md-1">Semana epidemiológica</th>
                @if (ViewBag.ILI == true)
            {					
                    <th data-bind="visible:EnableCHI() && NoActiveBOL()" class="col-md-1">
                            @(ViewBag.UsrCtry == 7 ? "Pobl." : "Tot. Cons.")
                    </th>

                    <th class="col-md-1">
                            @((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ? "ETI T." : "ETI Ext.")
                    </th>

                    <th data-bind="visible:EnableCHI()  && NoActiveBOL()" class="col-md-1">

                            @((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ? "ETI" : "ETI Emer.")
                    </th>
                }
                @if (ViewBag.SARI == true)
            {
                    <th class="col-md-1">Hosp. T</th>
                    <th class="col-md-1">@((ViewBag.UsrCtry == 7) ? "UPC" : "UCI") Tot</th>
                    <th class="col-md-1">Falle. T</th>
                    <th  data-bind="visible:EnableCHI() && NoActiveBOL()" class="col-md-1">Neumonía T</th>
                    <th  data-bind="visible:EnableCHI() && NoActiveBOL()" class="col-md-1">IRAG T</th>
                    <th  data-bind="visible:EnableCHI() && NoActiveBOL()" class="col-md-1">Vent. T</th>
                }
            </tr>
        </thead>
        <tbody>
            <!-- ko foreach: SummaryForYearItems -->
			<!--reemplazo-->
			<!-- fin reemplazo-->
            <tr style="cursor:pointer;" id="TotalBandeja" data-bind="attr: { 'data-week': StartDateOfWeek }, click: showEpiWeek">
                <td>
                        @*Semana*@ <span data-bind="text: ColEpiWeek" />  @*Modificacion por AM*@
                </td>
				@if (ViewBag.ILI == true)
			{				
				<td data-bind="visible:EnableCHI() && NoActiveBOL()">
					<span data-bind="value: ColETIDenoST"/>
                </td>				
				<td>
					<span data-bind="value: ColETINumST"/>
                </td>				
				<td data-bind="visible:EnableCHI() && NoActiveBOL()">
					<span data-bind="value: ColETINumEmerST"/>
                </td>
			}
                @if (ViewBag.SARI == true)
            {					
                    <td>
                            <span data-bind="text: ColHospTST" />
                    </td>					
                    <td >
                            <span data-bind="text: ColUCITST" />
                    </td>
                    <td>
                            <span data-bind="text: ColFalleTST" />
                    </td>					
					<td data-bind="visible:EnableCHI() && NoActiveBOL()">
						    <span data-bind="text: ColNeuTST" />						
                    </td>
					<td data-bind="visible:EnableCHI() && NoActiveBOL()">
                            <!--span data-bind="text: ColNeuTST" Columna IRAG T?? /-->
                    </td>
					<td data-bind="visible:EnableCHI() && NoActiveBOL()">
                            <span data-bind="text: ColVentTST" />
                    </td>
                }
            </tr>
            <!-- /ko -->
        </tbody>

    </table>

</div>

