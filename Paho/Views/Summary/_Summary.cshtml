@model Paho.Models.SummaryViewModel

<!-- ko with: app.Views.Summary -->
<div class="row" style="margin-bottom:15px; margin-top:1em;">
    <!--Country-->
    @if (Model.DisplayCountries)
    {
        <div class="col-md-5">
            <div class='input-group'>
                <span class="input-group-addon">
                    <!--País:-->
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelCountry"))
                </span>
                <select id="Pais" class="form-control" data-bind="options: activecountries,
                        optionsText: 'Name',
                        optionsValue: 'Id',
                        value: selectedCountryId,
                        optionsCaption: '@(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryFieldSelect"))',
                        event:{ change: ReloadHospitals}"></select>
            </div>
        </div>
    }
    <!--Hospital-->
    @if (Model.DisplayHospitals)
    {
        <div class="col-md-5">
            <div class='input-group'>
                <span class="input-group-addon">
                    <!--Establecimientos:-->
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelHospital"))
                </span>
                <select id="combHospital1" class="form-control" data-bind="options: hospitals,
                    optionsText: 'Name',
                    optionsValue: 'Id',
                    value: selectedHospitalId,
					event:{change: GetYearSummaryForYearItems}"></select>
                @*optionsCaption: 'Seleccione...',*@
            </div>
        </div>
    }
    <!--Fecha-->
    <div class="col-md-3">
        <div class='input-group'>
            <span class="input-group-addon">
                <!--Fecha:-->
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelDate"))
            </span>
            <input id="HospitalDate" type='text' class="form-control" data-bind='datepicker: HospitalDate, datepickerOptions: { beforeShowDay: PickLastDay, maxDate:@Model.DatePickerConfig, dateFormat:"@Model.DateFormatDP"}' />
        </div>
    </div>
    <!--EW-->
    <div class="col-md-2">
        <div class='input-group'>
            <span class="input-group-addon">
                <!--Semana Epi:-->
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelEpidemWeek"))
            </span>
            <input id="HospitalEW" type='text' readonly="readonly" class="form-control" data-bind="value: HospitalEW" />
        </div>
    </div>
    <!--EY-->
    <div class="col-md-2">
        <div class='input-group'>
            <span class="input-group-addon">
                <!--Año Epi:-->
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelEpidemYear"))
            </span>
            <input id="HospitalYE" type='text' readonly="readonly" class="form-control" data-bind="value: HospitalYE" />
        </div>
    </div>
</div>

<!-- Boton Confirmar-->
<div class="row" style="margin-bottom:15px; margin-top:1em;">
    <div class="col-md-3">
        <div class="btn-group">
            <!--<button id="search" type="button" class="btn btn-primary" data-bind="click: GetSummayItems">Confirmar</button> -->
            <button id="search" type="button" class="btn btn-primary" data-bind="click: GetSummayItems">@(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryButtonConfirm"))</button>
        </div>
    </div>
</div>

<!-- Cabecera Ingreso de Datos -->
<div class="row rowform" id="LabelSummary">
    <!-- Cabecera Grupos de Edad -->
