@model Paho.Models.SummaryViewModel
<!-- ko with: app.Views.Summary -->
<div class="row" style="margin-bottom:15px; margin-top:1em;">
       @if (Model.DisplayCountries)
       { 
           <div class="col-md-5">
               <div class='input-group'>
                   <span class="input-group-addon">
                       País:
                   </span>
                   <select id="Pais" class="form-control" data-bind="options: activecountries,
                          optionsText: 'Name',
                          optionsValue: 'Id',
                          value: selectedCountryId,
                          optionsCaption: 'Seleccione...',
                          event:{ change: ReloadHospitals}"></select>
               </div>
           </div> 
        }  
        @if (Model.DisplayHospitals)
        {
            <div class="col-md-5">
                <div class='input-group'>
               <span class="input-group-addon">
                   Establecimiento:
                </span>
                    <select id="combHospital1" class="form-control" data-bind="options: hospitals,
                       optionsText: 'Name',
                       optionsValue: 'Id',
                       value: selectedHospitalId,
					   event:{change: GetYearSummaryForYearItems}"></select>

                    @*optionsCaption: 'Seleccione...',*@
                </div>
            </div>
        }   
            <div class="col-md-3">
                <div class='input-group'>
                    <span class="input-group-addon">
                        Fecha:
                    </span>
                    <input id="HospitalDate" type='text' class="form-control" data-bind="datepicker: HospitalDate, datepickerOptions: { changeYear: true, beforeShowDay: PickLastDay, dateFormat:'dd/mm/yy', maxDate:@Model.DatePickerConfig}" />
                </div>
            </div>  
            <div class="col-md-2">
                <div class='input-group'>
                    <span class="input-group-addon">
                       Semana Epi:
                    </span>
                    <input id="HospitalEW" type='text' readonly="readonly" class="form-control" data-bind="value: HospitalEW" />
                </div>          
            </div> 
    <div class="col-md-2">
        <div class='input-group'>
            <span class="input-group-addon">
                Año Epi:
            </span>
            <input id="HospitalYE" type='text' readonly="readonly" class="form-control" data-bind="value: HospitalYE" />
        </div>
    </div> 

        </div>
