@model Paho.Models.SummaryViewModel

@{ 
    string LabelCountry = ((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelCountry");
    string FieldSelect = ((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryFieldSelect");
    string LabelHospital = ((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelHospital");
    string LabelDate = ((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelDate");
    string LabelEpidemWeek = ((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelEpidemWeek");
    string LabelEpidemYear = ((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelEpidemYear");
}

<style>
    .inputHN {
        height: 30px;
        padding-left: 5px;
        width: 55px;
        border: 1px solid #cccccc;
    }

    .inputTotalesHN {
        height: 30px;
        padding-left: 5px;
        width: 55px;
        border: solid #cccccc;
        background-color: #eeeeee;
    }

    .yearEpidem{
        background-color: #77dd77;
        /*background-color: #428bca;*/
        color: white;
    }
</style>

<!-- ko with: app.Views.Summary -->
<div class="row" style="margin-bottom:15px; margin-top:1em;">
    <!--Country-->
    @if (Model.DisplayCountries)
    {
        <div class="col-md-5">
            <div class='input-group'>
                <span class="input-group-addon">
                    @(LabelCountry)
                </span>
                <select id="Pais" class="form-control" data-bind="options: activecountries,
                        optionsText: 'Name',
                        optionsValue: 'Id',
                        value: selectedCountryId,
                        optionsCaption: '@(FieldSelect)',
                        event:{ change: ReloadHospitals}">
                </select>
            </div>
        </div>
    }
    <!--Hospital-->
    @if (Model.DisplayHospitals)
    {
        <div class="col-md-5">
            <div class='input-group'>
                <span class="input-group-addon">
                    @(LabelHospital)
                </span>
                <select id="combHospital1" class="form-control" data-bind="options: hospitals,
                    optionsText: 'Name',
                    optionsValue: 'Id',
                    value: selectedHospitalId,
					event:{change: GetYearSummaryForYearItems}">
                </select>
            </div>
        </div>
    }
    <!--Fecha-->
    <div class="col-md-3">
        <div class='input-group'>
            <span class="input-group-addon">
                @(LabelDate)
            </span>
            <input id="HospitalDate" type='text' class="form-control" data-bind='optionsAfterRender: showEpiWeek_AM, datepicker: HospitalDate, datepickerOptions: { beforeShowDay: PickLastDay, maxDate:@Model.DatePickerConfig, dateFormat:"@Model.DateFormatDP"}' />
        </div>
    </div>
    <!--EW-->
    <div class="col-md-2">
        <div class='input-group'>
            <span class="input-group-addon">
                <!--Semana Epi:-->
                @(LabelEpidemWeek)
            </span>
            <input id="HospitalEW" type='text' readonly="readonly" class="form-control" data-bind="value: HospitalEW" />
        </div>
    </div>
    <!--EY-->
    <div class="col-md-2">
        <div class='input-group'>
            <span class="input-group-addon">
                <!--Año Epi:-->
                @(LabelEpidemYear)
            </span>
            <input id="HospitalYE" type='text' readonly="readonly" class="form-control" data-bind="value: HospitalYE" />
        </div>
    </div>
</div>

<!-- Vigilancia -->
@if (ViewBag.SARI == true && ViewBag.ILI == true && (ViewBag.UsrCtry == 9 || ViewBag.UsrCtry == 15))
{
    <div class="row rowform">
        <div class="col-md-3 input-group" id="SurvContainer">
            <span class="input-group-addon">
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgCaselistVigTabContactLabelSurv"))
            </span>
            <label class="btn btn-default">
                <input type="radio" name="Surv" id="Surv" value="1" data-bind="checked: Surv" />  @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgCaselistVigTabContactLabelSari"))
            </label>
            <label class="btn btn-default">
                <input type="radio" name="Surv" id="Surv" value="2" data-bind="checked: Surv" />  @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgCaselistVigTabContactLabelIli"))
            </label>
        </div>
    </div>
}

<!-- Boton Confirmar-->
<div class="row" style="margin-bottom:15px; margin-top:1em;">
    <div class="col-md-3">
        <div class="btn-group">
            <!--<button id="search" type="button" class="btn btn-primary" data-bind="click: GetSummayItems">Confirmar</button> -->
            <button id="search" type="button" class="btn btn-primary" data-bind="click: GetSummayItems">@(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryButtonConfirm"))</button>
        </div>
    </div>
</div>

<!-- Cabecera Ingreso de Datos -->
<div class="row rowform" id="LabelSummary">
    <!-- Cabecera Grupos de Edad -->
    <div class="col-md-2">
        <span class="input-group-addon">
            @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelAgeGroup"))
        </span>
    </div>

    <!-- Cabecera Datos -->
    @if (ViewBag.ILI == true)
    {
        <!--Consultas-->
        <div class="@((ViewBag.UsrCtry == 3 || ViewBag.UsrCtry == 15) ?  "col-md-2" : "col-md-1")" data-bind="visible: EnableCHI() && ILI_OK()">
            <!--Consultas .F-->
            <div class='col-md-1 input-group' data-bind="visible: ActiveBOL() || ActiveHON()" style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3 ) ? "padding-left:0; padding-right:0;" : "")'>
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelTotalConsFemale"))
                </span>
            </div>
            <!--Consultas .M-->
            <div class='col-md-1 input-group' data-bind="visible: ActiveBOL() || ActiveHON()" style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3 ) ? "padding-left:0; padding-right:0;" : "")'>
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelTotalConsMale"))
                </span>
            </div>
            <!-- "Pobl." : "Tot. Cons."-->
            <div class="col-md-1 input-group" data-bind="visible: ActiveBOL() || ActiveCOS() || ActiveSUR() || ActiveHON()">
                <span class="input-group-addon">
                    @(ViewBag.UsrCtry == 7 ? (((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelPopulation")) : (((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelTotalCons")))
                </span>
            </div>
        </div>
        <!--ETI/ETI Ext.-->
        <div class="@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3 || ViewBag.UsrCtry == 15 ) ?  "col-md-2" : "col-md-1")" data-bind="visible: ILI_OK()">
            <!--ETI.F-->
            <div class='col-md-1 input-group' data-bind="visible: DisableCHI() || ActiveBOL() || ActiveHON()" style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3 ) ? "padding-left:0; padding-right:0;" : "")'>
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelILIFemale"))
                </span>
            </div>
            <!--ETI.M-->
            <div class='col-md-1 input-group' data-bind="visible: DisableCHI() || ActiveBOL() || ActiveHON()" style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3 ) ? "padding-left:0; padding-right:0;" : "")'>
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelILIMale"))
                </span>
            </div>
            <!--"ETI T." : "ETI Ext.-->
            <div class="col-md-1 input-group" data-bind="visible: NoActiveSUR() && NoActiveJAM() && NoActiveDOM() && NoActiveCAY()" style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3 || ViewBag.UsrCtry == 9) ? "padding-left:0; padding-right:0;" : "")"'>
                <span class="input-group-addon">
                    @((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ? (((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelILITotal")) : (((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelILIExt")))
                </span>
            </div>
        </div>
        <!--Emergency-->
        <div class="@((ViewBag.UsrCtry == 3 ) ?  "col-md-2" : "col-md-1")" data-bind="visible: EnableCHI() && NoActiveBOL() && !ActiveHON() && ILI_OK()">
            <!--ETI Emergency. F-->
            <div class='col-md-1 input-group' style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3 ) ? "padding-left:0; padding-right:0;" : "")' data-bind="visible:DisableCHI() || ActiveBOL() || NoActiveSUR() || NoActiveJAM()">
                <span class="input-group-addon" data-bind="visible: ActiveBOL()">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelILIEmerFem"))
                </span>
            </div>
            <!--ETI Emergency. M-->
            <div class='col-md-1 input-group' style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3 ) ? "padding-left:0; padding-right:0;" : "")' data-bind="visible:DisableCHI() || ActiveBOL() || NoActiveSUR() || NoActiveJAM()">
                <span class="input-group-addon" data-bind="visible: ActiveBOL()">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelILIEmerMaso"))
                </span>
            </div>
            <!--"ETI" : "ETI Emer. Total-->
            <div class="col-md-1 input-group" data-bind="visible: EnableCHI() && NoActiveSUR() && NoActiveJAM() && NoActiveDOM() && NoActiveCAY() ">
                <span class="input-group-addon">
                    @((ViewBag.UsrCtry == 7) ? (((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelILI")) : (((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelILIEmer")))
                </span>
            </div>
        </div>
    }
    @if (ViewBag.SARI == true)
    {
        <!-- Hospitalizacion -->
        if (ViewBag.UsrCtry == 15)
        {
            <div class="col-md-1_5" data-bind="visible: ActiveHON() && SARI_OK()">
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelHospitalizedFemale"))
                </span>
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelHospitalizedMale"))
                </span>
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelHospitalizedTotal"))
                </span>
            </div>
        }
        else
        {
            <div class="@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ?  "col-md-2" : "col-md-1")" data-bind="visible: SARI_OK() && Hospitalizacion_OK()">
                <div class='col-md-1 input-group' style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ? "padding-left:0; padding-right:0;" : "") ' data-bind="visible:DisableCHI() || ActiveBOL() || NoActiveSUR() || NoActiveJAM()">
                    <span class="input-group-addon font_12" data-bind="visible:DisableCHI() || ActiveBOL()">
                        @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelHospitalizedFemale"))
                    </span>
                </div>
                <div class='col-md-1 input-group' style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ? "padding-left:0; padding-right:0;" : "") ' data-bind="visible:DisableCHI() || ActiveBOL() || NoActiveSUR() || NoActiveJAM()">
                    <span class="input-group-addon font_12" data-bind="visible:DisableCHI() || ActiveBOL()">
                        @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelHospitalizedMale"))
                    </span>
                </div>
                <div class='col-md-1 input-group' style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ? "padding-left:0; padding-right:0;" : "")'>
                    <span class="input-group-addon">
                        @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelHospitalizedTotal"))
                    </span>
                </div>
            </div>
        }
        <!-- Hospitalizacion IRAG -->
        <div class="@((ViewBag.UsrCtry == 9) ?  "col-md-1" : "col-md-1_5")" data-bind="visible: (ActiveHON() || ActiveCOS()) && SARI_OK()">
            <span class="input-group-addon" data-bind="visible: HOS_IRAG_ShowCol_F()">
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelSARIFemale"))
            </span>
            <span class="input-group-addon" data-bind="visible: HOS_IRAG_ShowCol_M()">
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelSARIMale"))
            </span>
            <span class="input-group-addon" data-bind="visible: HOS_IRAG_ShowCol_T()">
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelSARITotal"))
            </span>
        </div>

        <!-- UCI -->
        if (ViewBag.UsrCtry == 15)
        {
            <div class="col-md-1_5" data-bind="visible: ActiveHON() && SARI_OK()">
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelUCIFemale"))
                </span>
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelUCIMale"))
                </span>
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelUCITotal"))
                </span>
            </div>
        }
        else
        {
            <div class="@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ?  "col-md-2" : "col-md-1")" data-bind="visible: SARI_OK() && UCI_OK()">
                <div class='col-md-1 input-group' style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ? "padding-left:0; padding-right:0;":"")' data-bind="visible:DisableCHI() || ActiveBOL() || NoActiveSUR() || NoActiveJAM()">
                    <span class="input-group-addon" data-bind="visible:DisableCHI() || ActiveBOL()">
                        @((ViewBag.UsrCtry == 7) ? (((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelICUFemale")) : (((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelUCIFemale")))
                    </span>
                </div>
                <div class='col-md-1 input-group' style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ? "padding-left:0; padding-right:0;":"")' data-bind="visible:DisableCHI() || ActiveBOL() || NoActiveSUR() || NoActiveJAM()">
                    <span class="input-group-addon" data-bind="visible:DisableCHI() || ActiveBOL()">
                        @((ViewBag.UsrCtry == 7) ? (((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelICUMale")) : (((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelUCIMale")))
                    </span>
                </div>
                <div class='col-md-1 input-group' style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ? "padding-left:0; padding-right:0;" : "")'>
                    <span class="input-group-addon" data-bind="visible: !ServPedriatic_SURI()">
                        @((ViewBag.UsrCtry == 7) ? (((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelICUTotal") ) : (((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelUCITotal")))
                    </span>
                </div>
            </div>
        }

        <!-- UCI IRAG -->
        <div class="@((ViewBag.UsrCtry == 9) ?  "col-md-1" : "col-md-1_5")" data-bind="visible: (ActiveHON() || ActiveCOS()) && SARI_OK()">
            <span class="input-group-addon" data-bind="visible: UCI_IRAG_ShowCol_F()">
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelUCISARIFemale"))
            </span>
            <span class="input-group-addon" data-bind="visible: UCI_IRAG_ShowCol_M()">
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelUCISARIMale"))
            </span>
            <span class="input-group-addon" data-bind="visible: UCI_IRAG_ShowCol_T()">
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelUCISARITotal"))
            </span>
        </div>

        <!-- Defunciones -->
        if (ViewBag.UsrCtry == 15)
        {
            <div class="col-md-1_5" data-bind="visible: ActiveHON() && SARI_OK()">
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelDeceasedFemale"))
                </span>
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelDeceasedMale"))
                </span>
                <span class="input-group-addon">
                    @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelDeceasedTotal"))
                </span>
            </div>
        }
        else
        {
            <div class="@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ?  "col-md-2" : "col-md-1")" data-bind="visible: SARI_OK()">
                <div class='col-md-1 input-group' style='@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ? "padding-left:0; padding-right:0;":"")' data-bind="visible:DisableCHI() || ActiveBOL() || NoActiveSUR() || NoActiveJAM() || ActiveHON()">
                    <span class="input-group-addon font_12" data-bind="visible: DisableCHI() || ActiveBOL()">
