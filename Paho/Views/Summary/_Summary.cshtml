        <!-- Defunciones IRAG -->
        <div class="@((ViewBag.UsrCtry == 9) ?  "col-md-1" : "col-md-1_5")" data-bind="visible: (ActiveHON() || ActiveCOS()) && SARI_OK()">
            <span class="input-group-addon" data-bind="visible: DEF_IRAG_ShowCol_F()">
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelDeceasedSARIFemale"))
            </span>
            <span class="input-group-addon" data-bind="visible: DEF_IRAG_ShowCol_M()">
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelDeceasedSARIMale"))
            </span>
            <span class="input-group-addon" data-bind="visible: DEF_IRAG_ShowCol_T()">
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelDeceasedSARITotal"))
            </span>
        </div>

        <!-- Neumonía -->
        <div class="col-md-1" data-bind="visible: SARI_OK() && EnableCHI() && NoActiveBOL() && NoActiveSUR() && NoActiveJAM() && NoActiveDOM() && NoActiveCAY() && !ActiveHON()">
            <span class="input-group-addon" data-bind="visible:false">
                <!--Neumonía F-->
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelPneumoniaFemale"))
            </span>
            <span class="input-group-addon" data-bind="visible:false">
                <!--Neumonía M.-->
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelPneumoniaMale"))
            </span>
            <span class="input-group-addon" data-bind="visible:EnableCHI()">
                <!--Neumonía T-->
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelPneumoniaTotal"))
            </span>
        </div>
        <!-- Ventilación -->
        <div class="col-md-1" data-bind="visible: SARI_OK() && EnableCHI() && NoActiveBOL() && NoActiveSUR() && NoActiveJAM() && NoActiveDOM() && NoActiveCAY() && !ActiveHON()">
            <span class="input-group-addon" data-bind="visible:false">
                <!--Vent.F-->
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelMechanicalVentFemale"))
            </span>
            <span class="input-group-addon" data-bind="visible:false">
                <!--Vent.M.-->
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelMechanicalVentMale"))
            </span>
            <span class="input-group-addon" data-bind="visible:EnableCHI()">
                <!--Vent.T-->
                @(((Paho.Controllers.SummaryController)this.ViewContext.Controller).getMsg("msgViewSummaryLabelMechanicalVentTotal"))
            </span>
        </div>
    }
</div>

<!-- ko foreach: SummayItems -->
<div class="row rowform" dstyle="margin-top:1em;">
    <div class="col-md-2">
        <span class="input-group-addon" data-bind="text: AgeGroupDescription">
        </span>
    </div>

    @if (ViewBag.ILI == true)
    {
        <!-- Atenciones -->
        <div class="@((ViewBag.UsrCtry == 3 || ViewBag.UsrCtry == 15 ) ?  "col-md-2" : "col-md-1")" data-bind="visible: EnableCHI() && ILI_OK()">
            <!--  -->
            <div class='col-md-1 input-group' data-bind="visible: ActiveBOL() || ActiveHON()">
                <input type='text' name="ETIDenoFem" id="ETIDenoFem" class="form-control" style="padding:6px;" data-bind="value: ETIDenoFem" />
            </div>
            <!--  -->
            <div class='col-md-1 input-group' data-bind="visible: ActiveBOL() || ActiveHON()">
                <input type='text' name="ETIDenoMaso" id="ETIDenoMaso" class="form-control" style="padding:6px;" data-bind="value: ETIDenoMaso" />
            </div>
            <!--  -->
            @if (ViewBag.UsrCtry == 15)
                {
                <div class="col-md-1 input-group" data-bind="visible: ActiveHON()">
                    <input type='text' name="ETIDenoST" id="ETIDenoST" class="form-control" style="padding:6px;" data-bind="value: ETIDenoST_HN" />
                </div>
            }
            else
            {
                <div class="col-md-1 input-group" data-bind="visible: ActiveSUR() || ActiveCOS() || ActiveBOL()">
                    <input type='text' name="ETIDenoST" id="ETIDenoST" class="form-control" style="padding:6px;" @((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3) ? "readonly='readonly'" : "") data-bind="value: ETIDenoST" />
                </div>
            }
        </div>
        <!-- Atenciones ETI -->
        <div class="@((ViewBag.UsrCtry == 7 || ViewBag.UsrCtry == 3 || ViewBag.UsrCtry == 15 ) ?  "col-md-2" : "col-md-1")" data-bind="visible: ILI_OK()">
            <!--  -->
            <div class='col-md-1 input-group' data-bind="visible:DisableCHI() || ActiveBOL() || ActiveHON()">
                <input type='text' name="ETINumFem" id="ETINumFem" class="form-control" style="padding:6px;" data-bind="value: ETINumFem" />
